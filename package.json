{
  "name": "material-symbols-expo",
  "type": "module",
  "version": "5.2.28",
  "description": "Material Symbols icon set for Expo.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "fetch:glyphs": "tsx ./scripts/fetch-glyphs.ts",
    "fetch:fonts": "tsx ./scripts/fetch-fonts.ts",
    "create-variants": "tsx ./scripts/create-variants.ts",
    "format": "prettier \"**/*.{ts,tsx,js,jsx}\" --write",
    "build:cjs": "webpack --config webpack.cjs.config.js",
    "build:esm": "webpack --config webpack.esm.config.js",
    "build": "npm run build:cjs && npm run build:esm && tsc",
    "update": "npm run clean && npm run fetch:glyphs && npm run fetch:fonts && npm run create-variants && npm run build"
  },
  "exports": {
    "default": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "paths": {
      "require": "./dist/paths.cjs",
      "import": "./dist/paths.js"
    },
    "icon-set": {
      "require": "./dist/icon-set.cjs",
      "import": "./dist/icon-set.js"
    },
    "icon": {
      "require": "./dist/icon.cjs",
      "import": "./dist/icon.js"
    }
  },
  "private": false,
  "peerDependencies": {
    "@expo/vector-icons": "^14.0.4",
    "expo-font": "^13.0.2",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@expo/vector-icons": "^14.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.3.12",
    "@types/xml2json": "^0.11.6",
    "@web-alchemy/fonttools": "^1.2.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "expo-font": "^13.0.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "ignore-loader": "^0.1.2",
    "node-fetch": "^3.3.2",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "xml2json": "^0.12.0"
  }
}
