{
  "name": "material-symbols-expo",
  "type": "module",
  "version": "5.2.38",
  "description": "Material Symbols icon set for Expo.",
  "main": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "clean:all": "rimraf dist src/assets",
    "fetch:glyphs": "tsx ./scripts/fetch-glyphs.ts",
    "fetch:fonts": "tsx ./scripts/fetch-fonts.ts",
    "create-variants": "tsx ./scripts/create-variants.ts",
    "format": "prettier \"**/*.{ts,tsx,js,jsx}\" --write",
    "build:cjs": "webpack --config webpack.cjs.config.js && tsc --outDir dist/cjs",
    "build:esm": "webpack --config webpack.esm.config.js && tsc --outDir dist/esm",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "update": "npm run clean:all && npm run fetch:glyphs && npm run fetch:fonts && npm run create-variants && npm run build"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./paths": {
      "require": "./dist/cjs/paths.cjs",
      "import": "./dist/esm/paths.js",
      "types": "./dist/esm/paths.d.ts"
    },
    "./glyphs": {
      "require": "./dist/cjs/glyphs.cjs",
      "import": "./dist/esm/glyphs.js",
      "types": "./dist/esm/glyphs.d.ts"
    },
    "./fonts": {
      "require": "./dist/cjs/fonts.cjs",
      "import": "./dist/esm/fonts.js",
      "types": "./dist/esm/fonts.d.ts"
    },
    "./components": {
      "require": "./dist/cjs/components.cjs",
      "import": "./dist/esm/components.js",
      "types": "./dist/esm/components.d.ts"
    },
    "./assets": "./dist/assets"
  },
  "private": false,
  "peerDependencies": {
    "@expo/vector-icons": "^14.0.4",
    "expo-font": "^13.0.2",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-flow": "^7.25.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@expo/vector-icons": "^14.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.3.12",
    "@types/xml2json": "^0.11.6",
    "@web-alchemy/fonttools": "^1.2.0",
    "babel-loader": "^9.2.1",
    "expo-font": "^13.0.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "xml2json": "^0.12.0"
  }
}
